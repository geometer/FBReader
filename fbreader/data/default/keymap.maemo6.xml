	<binding key="&lt;C&gt;" action="toc"/>
	<binding key="&lt;Ctrl&gt;+&lt;F&gt;" action="search"/>
	<binding key="&lt;F&gt;" action="search"/>
	<binding key="&lt;P&gt;" action="findPrevious"/>
